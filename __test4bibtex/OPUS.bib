@article{OPUS-lop-2011-Optim-Duarte-tabu-noPaley,
	Abstract = {Given a matrix of weights, the Linear Ordering Problem (LOP) consists of finding a permutation of the columns and rows in order to maximize the sum of the weights in the upper triangle. This well known NP-complete problem can also be formulated on a complete weighted graph, where the objective is to find an acyclic tournament that maximizes the sum of arc weights. The variant of the LOP that we target here was recently introduced and adds a cumulative non-linear propagation of the costs to the sum of the arc weights. We first review the previous methods for the LOP and for this variant with cumulative costs (LOPCC) and then propose a heuristic algorithm for the LOPCC, which is based on the Tabu Search (TS) methodology. Our method achieves search intensification and diversification through the implementation of both short and long term memory structures. Our extensive experimentation with 224 instances shows that the proposed procedure outperforms existing methods in terms of solution quality and has reasonable computing-time requirements.},
	Author = {Duarte, Abraham and Laguna, Manuel and Mart{\'\i}, Rafael},
	Date-Added = {2017-02-26 21:14:14 +0000},
	Date-Modified = {2017-02-26 21:14:14 +0000},
	Journal = {Computational Optimization and Applications},
	Number = {3},
	Pages = {697--715},
	Title = {{Tabu search for the linear ordering problem with cumulative costs}},
	Volume = {48},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1007/s10589-009-9270-5}}

